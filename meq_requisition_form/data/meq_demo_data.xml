<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="meq_request_demo_1" model="meq.request">
            <field name="item_name">Defibrillator</field>
            <field name="item_code">EQP-001</field>
            <field name="item_type">disposable</field>
            <field name="urgency">low</field>
            <field name="usage_location">ICU</field>
            <field name="uom">PC</field>
            <field name="equipment_month">2</field>
            <field name="quantity">1</field>
